{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended-type-checked",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": true
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "root": true,
  "rules": {
    "prettier/prettier": ["error"]
  },
  "ignorePatterns": ["dist", "node_modules"]
}
